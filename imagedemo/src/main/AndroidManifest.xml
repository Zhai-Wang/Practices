<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shmj.mouzhai.imagedemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PrimaryColorActivity" />
        <activity android:name=".ColorMatrixActivity" />
        <activity android:name=".PixelsActivity" />
        <activity android:name=".ImageMatrixActivity" />
        <activity android:name=".XfermodeActivity" />
        <activity android:name=".ReflectActivity" />
        <activity android:name=".MeshActivity"></activity>
    </application>

</manifest>